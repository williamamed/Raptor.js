  "repository": {
    "type": "git",
    "url": "git+https://github.com/srod/node-minify.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "compressor",
    "minify",
    "minifier",
    "yui",
    "gcc",
    "google",
