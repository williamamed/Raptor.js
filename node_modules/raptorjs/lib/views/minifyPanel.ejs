<div class="raptorjs-node-debug" >

	<div class="raptorjs-node-debug-btn" >
		<img height="30" src="/public/Raptor/img/monitor.png" >
	</div>
	<div class="raptorjs-node-debug-data" >
		<%- R.pluginRequest('raptor_profiler') %>
	</div>
		<style type="text/css">
			.raptorjs-node-debug{
				position:fixed;right: 5px; background:url(/public/Raptor/img/md.png);bottom: 5px;height:45px;overflow:hidden;box-shadow: 6px 4px 5px 0px rgba(0,0,0,0.75);border:1px solid #DCDCDC
			}
			.raptorjs-node-debug-btn{
				background-color: #372b53;width:45px;height:45px;float:left;text-align: center;vertical-align:middle;padding:6px;cursor: pointer
			}
			.raptorjs-node-debug-btn img{
				overflow:visible
			}
			.raptorjs-node-debug-data{
				float:left;padding:7px
			}
		</style>

		<script>
		(function(){

			var Debug={
				middle:false,
				open:true,
				init:function(){
					var width=$('.raptorjs-node-debug').width()
					
					var cle=setTimeout(function(){
								Debug.middle=true;
								Debug.open=false;
								$('.raptorjs-node-debug').animate({
									width:45,
									
								},1000,function(){
									Debug.middle=false;
								})
							},10*1000)

					$('.raptorjs-node-debug-btn').on('click',function(){
						if(cle)
							clearTimeout(cle)
						if(!Debug.middle){
							Debug.middle=true;
							if(!Debug.open){
								Debug.open=true;
								$('.raptorjs-node-debug').animate({
									width:width+5,
									
								},1000,function(){
									Debug.middle=false;
								})
							}else{
								Debug.open=false;
								$('.raptorjs-node-debug').animate({
									width:45,
									
								},1000,function(){
									Debug.middle=false;
								})
							}
						}
					})
				}
			}

			if(!window.$){
	            var script2 = document.createElement('script');
	             script2.onload = script2.onreadystatechange = script2.onerror = null;
	             script2.type = 'text/javascript';

	             script2.onload = function(){
	                     Debug.init()
	             };
	             script2.src = '/public/bootstrap/js/jquery.js';
	             document.getElementsByTagName('head')[0].appendChild(script2);
	        }else
	            Debug.init();
		})()
    
	</script>
</div>