<link href="/public/@raptorjs/raptor-panel/css/portal.css" rel="stylesheet">
<!-- Socket.IO -->
<script src="/socket.io/socket.io.js"></script>

<script>
    R = {
        message: function (message) {
            $mdToast=angular.element(document.body).injector().get('$mdToast')
            return $mdToast.show(
                    $mdToast.simple()
                        .action('Cerrar')
                        .highlightAction(true)
                        .textContent(message)
                        .position('top right')
                        .hideDelay(9000))
                    

        }
    }

    
</script>